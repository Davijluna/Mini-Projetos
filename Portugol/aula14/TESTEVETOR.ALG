Algoritmo "TesteVetor"

Var
   v :vetor[1..6] de inteiro
   c: inteiro

Inicio
      para c <- 1 ate 6 faca
             escreval("Digite o contador ", c, " valor")
             leia(v[c])
      fimpara
      
      para c <- 1 ate 6 faca
             escreval("[",v[c],"]")
      fimpara


Fimalgoritmo